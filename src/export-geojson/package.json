{
  "name": "detect-dirty-tiles",
  "version": "1.0.0",
  "description": "Export smoothed linestrings from lake labels as GeoJSON",
  "main": "export-smoothed-geojson.js",
  "scripts": {
    "start": "node export-smoothed-geojson.js"
  },
  "author": "lukasmartinelli",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.1.4",
    "pg": "^6.0.4",
    "pg-query-stream": "^1.0.0",
    "q": "^1.4.1",
    "stream": "0.0.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        4
      ],
      "no-console": [
        0
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  }
}
